<!doctype html>
<html class="no-js" lang="">
<head>
	<meta charset="utf-8">
	<meta http-equiv="x-ua-compatible" content="ie=edge">
	<title>{{ page['meta']['title'] }}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	
	<!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
	<link rel="icon" href="favicon.ico">
	<link rel="apple-touch-icon-precomposed" sizes="57x57" href="apple-icon-57x57-precomposed.png" />
	<link rel="apple-touch-icon-precomposed" sizes="72x72" href="apple-icon-72x72-precomposed.png" />
	<link rel="apple-touch-icon-precomposed" sizes="114x114" href="apple-icon-114x114-precomposed.png" />
	<link rel="apple-touch-icon-precomposed" sizes="144x144" href="apple-icon-144x144-precomposed.png" />



	<link rel="stylesheet" href="/vendor/Summernote/Summernote/dist/summernote.{{ _v }}.css">
	<link rel="stylesheet" href="/ui/style.{{ _v }}.css">
	<link rel="stylesheet" href="/ui/admin/_style.{{ _v }}.css">
	{% for row in page['css'] %}
		<link rel="stylesheet" href="{{ row }}">
	{% endfor %}
	{% if page['template_css'] %}
		<link rel="stylesheet" href="{{ page['template_css'] }}">
	{% endif %}
	<style type="text/css">
		{% for item in _css %}
		{{ item|raw() }};
		{% endfor %}
	</style>
	
	<script src="/vendor/components/modernizr/modernizr.{{ _v }}.js"></script>
</head>
<body >

	{% include "_header.twig" ignore missing %}
	
	
	
	<section class="container" id="page-content">
		{% include page['template'] %}
	</section>
	
	{% include "_footer.twig" ignore missing  %}


<script src="/ui/javascript.{{ _v }}.js"></script>
<script src="/ui/admin/_javascript.{{ _v }}.js"></script>


{% for row in page['js'] %}
	<script src="{{ row }}"></script>
{% endfor %}
{% if page['template_js'] %}
	<script src="{{ page['template_js'] }}"></script>
{% endif %}
	
	
	<script type="text/javascript">
		{% for item in _javascript %}
		{{ item|raw() }};
		{% endfor %}
	</script>


	<style>
		.agenda-item.agenda-item-vertical {

		}
		.staff-badge-, .agenda-item.agenda-staff-item- {
			color:#fff;
			background-color: #777777;
		}
		{% for row in _staff %}
			.staff-badge-{{ row['ID'] }} {
				{% for key,style in row['badge_style'] %}
					{{ key }}: {{ style }};
				{% endfor %}
			}

			.agenda-item.agenda-staff-item-{{ row['ID'] }} {
				{{ row['badge_style_rendered']['normal'] }}
				border: 1px solid {{ row['badge_style']['background-color'] }};
			}
			.agenda-item.agenda-staff-item-{{ row['ID'] }}:hover {
				{{ row['badge_style_rendered']['hover'] }}
				border: 1px solid {{ row['badge_style_hover']['background-color'] }};
			}
			.agenda-item.agenda-staff-item-{{ row['ID'] }}.active {
				{{ row['badge_style_rendered']['active'] }}
				border: 1px solid {{ row['badge_style_active']['background-color'] }};
			}
			.agenda-item.agenda-staff-item-{{ row['ID'] }}.current {
				{{ row['badge_style_rendered']['current'] }}
				border: 1px solid {{ row['badge_style_current']['background-color'] }};
			}


			.timeslot-item-staff-{{ row['ID'] }} {
				background: /* On "top" */
						repeating-linear-gradient(
								45deg,
								transparent,
								transparent 5px,
								#f1f1f1 5px,
								#f1f1f1 10px
						),
							/* on "bottom" */
						linear-gradient(
								to bottom,
								rgba({{ row['badge_style_rgb']['R'] }},{{ row['badge_style_rgb']['G'] }},{{ row['badge_style_rgb']['B'] }},0.1),
								rgba({{ row['badge_style_rgb']['R'] }},{{ row['badge_style_rgb']['G'] }},{{ row['badge_style_rgb']['B'] }},0.12)
						) !important;
			}



		{% endfor %}
	</style>


{% if page['template_jtmpl'] %}
	{% include page['template_jtmpl'] %}
{% endif %}

{% include 'template/template.jtmpl' %}



<div class="modal fade" id="systemTimers-container" tabindex="-1" role="dialog" aria-labelledby="SystemTimers" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" id="myModalLabel">Debug</h4>
			</div>
			<div class="modal-body" style="padding:0;">
				<article class="table-responsive">
					<table id='systemTimers' class="table font-small">
					
					</table>
				</article>
			</div>
		</div>
	</div>
</div>


<div class="modal fade" id="modal-window" role="dialog" aria-labelledby="modal-window" aria-hidden="true"> </div>
<div class="modal fade" id="modal-form" role="dialog" aria-labelledby="modal-form" aria-hidden="true"> </div>



<div class="loadingmask fa" id="loadingmask" >
	<i class="fa fa-cog fa-spin"></i>
</div>
	
	
	<div id="system-timer-btn" >
		<a href="#systemTimers-container" data-toggle="modal"><i class="fa fa-clock-o"></i></a>
	</div>
